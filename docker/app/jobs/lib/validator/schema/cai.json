{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "asset_type"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "//cloudresourcemanager.googleapis.com/organizations/684587186245"
      ],
      "pattern": "^(.*)$"
    },
    "asset_type": {
      "$id": "#/properties/asset_type",
      "type": "string",
      "title": "The Asset_type Schema",
      "default": "",
      "examples": [
        "cloudresourcemanager.googleapis.com/Organization"
      ],
      "pattern": "^(.*)$"
    },
    "resource": {
      "$id": "#/properties/resource",
      "type": "object",
      "title": "The Resource Schema",
      "required": [
        "data"
      ]
    }
  }
}
